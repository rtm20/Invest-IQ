{
  "version": 2,
  "name": "ai-startup-analyst",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "env": {
    "NEXT_PUBLIC_ENABLE_REAL_AI": "false",
    "NEXT_PUBLIC_ENABLE_MOCK_MODE": "true",
    "NEXT_PUBLIC_ENABLE_DEBUG_LOGS": "false"
  },
  "regions": ["iad1"]
}
